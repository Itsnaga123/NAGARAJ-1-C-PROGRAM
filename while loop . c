#include <stdio.h>

int main() {
    int i = 1;  // Initialize variable

    // Start of while loop
    while (i <= 5) {  // Condition: loop will run as long as i is less than or equal to 5
        printf("i = %d\n", i);  // Print the value of i
        i++;  // Increment i
    }
    
    return 0;
}
